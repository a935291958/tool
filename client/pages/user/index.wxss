/* pages/about/index.wxss */
image {
  width: 100%;
  max-width: 100%;
}
.bgfff {
  background: #fff;
}
page {
  background: #f9f9f9;
}
.topImg > image {
  width: 90%;
  margin-left: auto;
  margin-right: auto;
  display: block;
  padding-top: 30rpx;
  -webkit-border-radius: 10rpx;
  -moz-border-radius: 10rpx;
  -ms-border-radius: 10rpx;
  -o-border-radius: 10rpx;
  border-radius: 10rpx;
}
.topHosName {
  display: table;
  margin-left: auto;
  margin-right: auto;
  padding-top: 1em;
  padding-bottom: .6em;
}
.topHosName image {
  width: 60rpx;
  height: 60rpx;
  display: inline-block;
  float: left;
}
.topHosName text {
  float: left;
  line-height: 60rpx;
  color: #565656;
  font-size: 18px;
  margin-left: .5em;
}
.info {
  margin-top: 30rpx;
}
.infoLeft,
.infoRig {
  display: inline-block;
}
.infoRow {
  line-height: 100rpx;
  width: 96%;
  margin-left: auto;
  margin-right: auto;
  border-bottom: 1px solid #f0f0f0;
  position: relative;
}
.more {
  height: 40rpx ;
  width: 20rpx;
  right: 1%;
  top: 50%;
  position: absolute;
  margin-top: -20rpx;
}
.infoLeft {
  width: 30%;
  color: #555555;
  font-size: 16px;
}
.infoRig {
  color: #888888;
  font-size: 16px;
  width: 70%;
}
.tel .infoRig {
  color: #54c9c3;
}
.jianjie {
  box-sizing: content-box;
}
.jianjie .infoLeft {
  vertical-align: top;
  display: -webkit-box;
  float: left;
}
.jianjie .infoRig {
  padding-top: 20rpx;
  line-height: 50rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 5;
  overflow: hidden;
}
.jianjie .infoRig.showAll {
  -webkit-line-clamp: inherit;
}
.brand {
  margin-top: 30rpx;
  width: 96%;
