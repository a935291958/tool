img.a {
  position: absolute;
  top: 0px;
  left: 50%;
  margin-left: -320px;
  opacity: 0.8;
}
.main:after,
.wp:after {
  content: "";
  display: block;
  clear: both;
  height: 0;
  visibility: hidden;
}
.wp {
  width: 100%;
  max-width: 640px;
  min-width: 320px;
  margin-left: auto;
  margin-right: auto;
}
.cp {
  cursor: pointer;
}
.banner img {
  display: block;
}
@media only screen and (min-width: 640px) {
  .wp {
    max-width: 640px;
  }
  html {
    font-size: 20px;
  }
}
@media only screen and (max-width: 600px) {
  .wp {
    max-width: 600px;
  }
  html {
    font-size: 100%;
  }
}
@media only screen and (max-width: 480px) {
  .wp {
    max-width: 480px;
  }
  html {
    font-size: 75%;
  }
}
@media only screen and (max-width: 360px) {
  .wp {
    max-width: 360px;
  }
  html {
    font-size: 62.5%;
  }
}
